{
    "manifest_version": 2,

    "name": "FindYourWay",
    "description": "",
    "version": "1.0",

    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "content_scripts":[{
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }]
}
